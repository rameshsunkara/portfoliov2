{
  "root": true,
  "extends": "@upstatement/eslint-config/react",
  "parserOptions": {
    "requireConfigFile": false,
    "sourceType": "module",
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "jsx": true
    },
    "babelOptions": {
      "configFile": false,
      "babelrc": false,
      "parserOpts": {
        "plugins": [
          "jsx"
        ]
      }
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
